<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='authentication'>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name='com.google.gwt.uibinder.UiBinder'/>
    <inherits name='com.sencha.gxt.ui.GXT' />

    <inherits name="com.google.gwt.json.JSON" />
    <inherits name="com.google.gwt.http.HTTP" />
    <inherits name="com.allen_sauer.gwt.log.gwt-log-OFF" />
    <inherits name="plupload.plupload" />
    <inherits name='com.mvp4g.Mvp4gModule' />

    <!-- Inherit the default GWT style sheet.  You can change       -->
    <!-- the theme of your GWT application by uncommenting          -->
    <!-- any one of the following lines.                            -->
    <!--<inherits name='com.google.gwt.user.theme.standard.Standard'/>-->
     <!--<inherits name='com.google.gwt.user.theme.chrome.Chrome'/>-->
    <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
    <stylesheet src="reset.css" /> <!-- <<<< Sets up the GXT default stylesheet -->

    <extend-property name="locale" values="en" />
    <extend-property name="locale" values="nb" />
    <extend-property name="locale" values="nl" />
    <extend-property name="locale" values="sv" />
    <extend-property name="locale" values="da" />
    <extend-property name="locale" values="fr" />
    <extend-property name="locale" values="de" />

    <set-property-fallback name="locale" value="en" />

    <!-- Other module inherits                                      -->

    <!-- Specify the app entry point class.                         -->
    <entry-point class='net.bees.frontend.client.authentication'/>

    <!-- Specify the paths for translatable code                    -->
    <source path='client'/>
    <source path='shared'/>

    <set-property name="gwt.logging.consoleHandler" value="ENABLED" />
    <set-property name="gwt.logging.logLevel" value="FINE" />

</module>
